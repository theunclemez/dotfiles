# GIMP gimprc
# 
# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(swap-compression "balanced")
(use-opencl yes)
(default-image
    (width 3840)
    (height 2160)
    (unit pixels)
    (xresolution 144)
    (yresolution 144)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type background)
    (comment "Created by Uncle Mez"))
(thumbnail-filesize-limit 1024k)
(import-add-alpha yes)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(export-file-type tif)
(snap-distance 6)
(marching-ants-speed 100)
(monitor-xresolution 112)
(monitor-yresolution 115)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas yes)
    (snap-to-path yes)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(move-tool-changes-active yes)
(icon-theme "Symbolic-High-Contrast")
(icon-size small)
(image-resize-layer-set all)
(layer-new-name "vignette")
(layer-add-mask-type channel)
(selection-border-radius 45)
(selection-border-style hard)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


# end of gimprc
