# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(swap-path "${gimp_temp_dir}")
(swap-compression "balanced")
(brush-path "${gimp_dir}/brushes:${gimp_data_dir}/brushes:/home/unclemez/Documents/Addons/For Gimp/Brushes/Zipped")
(brush-path-writable "${gimp_dir}/brushes:/home/unclemez/Documents/Addons/For Gimp/Brushes/Zipped")
(default-image
    (width 5760)
    (height 3240)
    (unit pixels)
    (xresolution 144)
    (yresolution 144)
    (resolution-unit inches)
    (image-type rgb)
    (precision u8-gamma)
    (color-managed yes)
    (color-profile NULL)
    (fill-type transparent)
    (comment "Created by Uncle Mez"))
(undo-preview-size medium)
(thumbnail-size none)
(thumbnail-filesize-limit 1024k)
(import-add-alpha yes)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(export-metadata-exif no)
(export-metadata-xmp no)
(export-metadata-iptc no)
(snap-distance 6)
(marching-ants-speed 100)
(monitor-xresolution 136)
(monitor-yresolution 135)
(default-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(default-fullscreen-view
    (show-menubar yes)
    (show-statusbar yes)
    (show-rulers yes)
    (show-scrollbars yes)
    (show-selection yes)
    (show-layer-boundary yes)
    (show-canvas-boundary yes)
    (show-guides yes)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid yes)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode default)
    (padding-color (color-rgb 1 1 1))
    (padding-in-show-all no))
(move-tool-changes-active yes)
(theme "Dark")
(icon-theme "Symbolic-High-Contrast")
(icon-size small)
(action-history-size 112)
(layer-add-mask-type selection)
(selection-grow-radius 5)
(selection-border-radius 3)
(selection-border-style feathered)
(selection-border-edge-lock yes)
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
